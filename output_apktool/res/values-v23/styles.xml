        <item name="android:colorBackground">@color/sud_glif_background_color_dark</item>
        <item name="android:editTextStyle">@style/SudEditText</item>
        <item name="android:alertDialogTheme">@style/SudAlertDialogTheme</item>
        <item name="android:windowLightStatusBar">false</item>
        <item name="alertDialogTheme">@style/SudAlertDialogThemeCompat</item>
        <item name="sucLightStatusBar">?android:windowLightStatusBar</item>
        <item name="sudBackgroundBaseColor">?android:colorBackground</item>
