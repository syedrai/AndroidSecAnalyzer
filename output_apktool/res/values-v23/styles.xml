        <item name="android:colorBackground">@color/sud_glif_background_color_light</item>
        <item name="android:editTextStyle">@style/SudEditText</item>
        <item name="android:alertDialogTheme">@style/SudAlertDialogTheme.Light</item>
        <item name="android:windowLightStatusBar">true</item>
        <item name="alertDialogTheme">@style/SudAlertDialogThemeCompat.Light</item>
        <item name="sucLightStatusBar">?android:windowLightStatusBar</item>
        <item name="sudBackgroundBaseColor">?android:colorBackground</item>
